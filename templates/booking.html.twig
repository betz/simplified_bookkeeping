{#
/**
 * @file booking.html.twig
 * Default theme implementation to present Booking data.
 *
 * This template is used when viewing Booking pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_booking()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('booking') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
